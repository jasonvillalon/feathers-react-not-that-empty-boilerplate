{
  "name": "closetph",
  "description": "closetph",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Jason Villalon",
    "email": "jasonvillalon@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "eslint": "eslint client/. common/. config/. src/. test/. --ext .js,.jsx",
    "start": "concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "dev": "concurrently --kill-others \"npm run start-dev\" \"npm run start-dev-api\"",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "build": "better-npm-run build",
    "build-dlls": "webpack --colors --display-error-details --config webpack/vendor.config.js",
    "postinstall": "concurrently \"npm run build\" \"npm run build-dlls\"",
    "lint": "eslint -c .eslintrc api src",
    "test": "karma start",
    "test-node": "jest $(find api -name '*-test.js')",
    "test-node-watch": "jest $(find api -name '*-test.js') --watch",
    "docker-build": ". ./docker/build.sh",
    "docker-dev": ". ./docker/run.dev.sh",
    "docker-prod": ". ./docker/run.prod.sh",
    "lighthouse-report": "LIGHTHOUSE_CHROMIUM_PATH=$(which google-chrome) lighthouse http://localhost:8080"
  },
  "betterScripts": {
    "start-prod": {
      "command": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack/prod.config.js --progress --colors",
      "env": {
        "NODE_PATH": "./client",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "NODE_ENV=devserver ./webpack/webpack-dev-server.js --config ./webpack/dev.config.js --content-base public/dist",
      "env": {
        "NODE_PATH": "./client",
        "NODE_ENV": "devserver",
        "PORT": 3000,
        "APIPORT": 3030,
        "WEBPACK_DLLS": 1
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "devserver",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./client",
        "NODE_ENV": "devserver",
        "PORT": 3000,
        "APIPORT": 3030,
        "WEBPACK_DLLS": 1
      }
    },
    "build": {
      "command": "webpack --colors --display-error-details --config=webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
