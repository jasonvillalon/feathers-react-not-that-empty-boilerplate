{
  "presets": [
    "react",
    "es2015",
    "stage-0",
    "flow"
  ],
  "plugins": [
    "transform-runtime",
    "add-module-exports",
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "sourceMaps": "dev"
    },
    "production": {
      "plugins": [
       "transform-react-constant-elements",
       "transform-react-inline-elements"
      ]
    }
  }
}
